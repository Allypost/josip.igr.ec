{
    "prettier.documentSelectors": [
        "**/*.astro"
    ],
    "[astro]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "eslint.validate": [
        "javascript",
        "javascriptreact",
        "typescript",
        "typescriptreact",
        "astro",
        "svelte",
        "vue",
    ],
    "tailwindCSS.classAttributes": [
        "class",
        "className",
        "ngClass",
        "class:list"
    ]
}